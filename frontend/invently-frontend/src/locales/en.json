    "guide": {
      "title": "Complete CSV Format Guide",
      "columnsIntro": "The CSV file has 10 columns. Here's what each column means and how to use it:",
      "category": {
        "title": "Category",
        "required": true,
        "description": "The category path. Use \" > \" to create nested categories (parent > child).",
        "example1": "Electronics",
